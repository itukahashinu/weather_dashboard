{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "rewrites": [
      { "source": "/api/updateWeather", "destination": "/api/updateWeather" },
      { "source": "/api/weather", "destination": "/api/weather" },
      { "source": "/(.*)", "destination": "/index.html" }
    ]
  }